{
  "manifest_version": 3,
  "name": "Avanza Courtagekalkylator",
  "version": "1.0.0",
  "description": "Visar courtagekostnad per courtageklass för givet affärsbelopp på Avanzas ordersidor.",
  "permissions": [],
  "host_permissions": [
    "https://www.avanza.se/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.avanza.se/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ]
}
